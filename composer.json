{
    "name": "imponeer/smarty-foreachq",
    "description": "Rewritten Smarty foreach variant that was invented for use in xoops, but nowadays is used in some other PHP based CMS'es",
    "type": "library",
    "require": {
        "php": "^8.3",
        "smarty/smarty": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "symfony/var-dumper": "^7.3",
        "mikey179/vfsstream": "^1.6",
        "squizlabs/php_codesniffer": "^3.13"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Raimondas Rimkeviƒçius",
            "email": "github@mekdrop.name"
        }
    ],
    "autoload": {
        "psr-4": {
            "Imponeer\\Smarty\\Extensions\\ForeachQ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imponeer\\Smarty\\Extensions\\ForeachQ\\Tests\\": "tests/"
        }
    },
    "keywords": [
        "smarty",
        "xoops",
        "foreach",
        "smarty-plugins"
    ],
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf"
    }
}
